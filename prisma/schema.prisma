generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model produto {
  id         Int    @id @default(autoincrement())
  nome       String
  quantidade Int
  preco      Float
}

model user {
  id    Int      @id @default(autoincrement())
  nome  String
  email String   @unique(map: "User_email_key")
  senha String
  data  DateTime @default(now())
}
